
<div class="container">
  <div class="card" id="{{user.id}}">
    <img class="card-img-top" src="http://via.placeholder.com/286x180" alt="Card image cap">
    <div class="card-body">
      <h5 class="card-title">{{user.firstName}} {{user.lastName}}</h5>
      <p class="card-text">{{user.email}}</p>
      <a href="/update-profile" class="btn btn-primary">Update Profile</a>
      <a href="#" class="btn btn-primary">My Charities</a>
    </div>
  </div>
</div>
