


<div class="container dallasOrgs">
  <div class="row">
    {{#each allCharities}}
      <div class="col-md-6 orgCard">
        <div class="card">
          <img class="card-img-top" src="http://via.placeholder.com/286x180" alt="charity_image">
          <div class="card-body">
            <h4 class="card-title">{{name}}</h4>
            <p>{{description}}</p>
            <a href="{{url}}">{{name}}</a>
            <p>{{phone_number}}</p>
          </div>
        </div>
      </div>
    {{/each}}
  </div>
</div>
